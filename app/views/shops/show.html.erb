<div class = 'row'>
  <div class = 'small-12 columns'>
    <%= image_tag(@shop.shop_image) if @shop.shop_image? %>
  </div>
  <% flash.each do |key,value| %>
    <div class = "alert-box round <%= flash_class(key) %>">
      <%= value %>
    </div>
  <% end %>
</div>
<%= render 'shared/shop_details', shop: @shop %>
